{
	"name": "carrooi/doctrine",
	"type": "library",
	"description": "Base classes for doctrine based on kdyby/doctrine",
	"keywords": ["doctrine", "database", "nette"],
	"license": ["MIT"],
	"authors": [
		{
			"name": "David Kudera",
			"homepage": "http://davidkudera.com",
			"email": "kudera.d@gmail.com"
		}
